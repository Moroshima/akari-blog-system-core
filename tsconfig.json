{
  "include": [
    "app.ts",
    "config/**/*",
    "controllers/**/*",
    "dao/**/*",
    "models/**/*",
    "routes/**/*",
    "services/**/*",
    "utils/**/*"
  ],
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
